{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "modules": "commonjs"
        }
      }
    ],
    "@babel/preset-flow",
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "react-hot-loader/babel"
    ], [
      "@babel/plugin-proposal-class-properties"
    ], [
      "@babel/plugin-transform-runtime"
    ]
  ]
}
